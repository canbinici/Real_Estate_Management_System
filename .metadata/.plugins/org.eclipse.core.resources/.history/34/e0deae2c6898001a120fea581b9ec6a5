import java.sql.Date;

public abstract class AbstractProperty {
	
	protected int id;
	protected String type;
	protected String status;
	protected Date date;
	protected String title;
	protected String address;
	
	public AbstractProperty(int id){
		this.id = id;
	}
	
	public AbstractProperty(int id, String type, String status, Date date, String title, String address){
		this.id = id;
		this.type = type;
		this.status = status;
		this.date = date;
		this.title = title;
		this.address = address;
		
	}

}
